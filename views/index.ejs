      /* Promo Banner Styles */
      .promo-banner {
        display: block;
        text-decoration: none;
        padding: 24px 16px;
        text-align: center;
        position: relative;
        overflow: hidden;
        cursor: pointer;
        transition: transform 0.2s ease, box-shadow 0.2s ease;
