        position: relative;
      }
      .birthday-banner::before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: url("data:image/svg+xml,%3Csvg width='100' height='100' viewBox='0 0 100 100' xmlns='http://www.w3.org/2000/svg'%3E%3Ccircle cx='10' cy='10' r='2' fill='rgba(255,255,255,0.15)'/%3E%3Ccircle cx='50' cy='30' r='1.5' fill='rgba(255,255,255,0.1)'/%3E%3Ccircle cx='80' cy='60' r='2' fill='rgba(255,255,255,0.12)'/%3E%3Ccircle cx='30' cy='70' r='1' fill='rgba(255,255,255,0.1)'/%3E%3Ccircle cx='90' cy='20' r='1.5' fill='rgba(255,255,255,0.15)'/%3E%3C/svg%3E");
        pointer-events: none;
        animation: sparkle 3s ease-in-out infinite;
      }
      @keyframes sparkle {
        0%, 100% { opacity: 1; }
        50% { opacity: 0.7; }
      }
      .birthday-banner:hover {
        box-shadow: 0 4px 25px rgba(255, 107, 107, 0.5);
      }
      .birthday-banner .promo-tagline {
        color: #fff5f5;
      }
      .birthday-banner .promo-badge {
        background: linear-gradient(135deg, #fff 0%, #ffe8e8 100%);
        color: #e74c3c;
        font-weight: 800;
      }
      .promo-content {
        position: relative;
        z-index: 1;
        max-width: 600px;
        margin: 0 auto;
      }
      .promo-emoji {
        font-size: 3rem;
        margin-bottom: 10px;
        animation: bounce 2s ease-in-out infinite;
      }
      @keyframes bounce {
        0%, 100% { transform: translateY(0); }
        50% { transform: translateY(-6px); }
      }
      .promo-title {
        color: white;
        font-size: 1.4rem;
        font-weight: 800;
        margin: 0 0 8px 0;
        text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.2);
      }
      .promo-tagline {
        color: #ffd6f7;
        font-size: 0.95rem;
        margin: 0 0 12px 0;
        font-weight: 500;
        line-height: 1.5;
      }
      .promo-badge {
        display: inline-block;
        background: linear-gradient(135deg, #ff6b9d, #ff8a65);
        color: white;
        padding: 6px 14px;
        border-radius: 20px;
        font-size: 0.75rem;
        font-weight: 700;
        box-shadow: 0 3px 10px rgba(0, 0, 0, 0.2);
        transition: transform 0.2s ease, background 0.2s ease;
      }
      @keyframes pulse {
        0%, 100% { transform: scale(1); }
        50% { transform: scale(1.03); }
      }
      @media (max-width: 768px) {
        .promo-banner {
          padding: 20px 12px;
        }
        .promo-emoji {
          font-size: 2.5rem;
        }
        .promo-title {
          font-size: 1.2rem;
        }
        .promo-tagline {
          font-size: 0.85rem;
        }
